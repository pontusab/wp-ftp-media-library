msgid "WP FTP Media"
msgstr "WP FTP 媒體管理"

#: fields.php:24
msgid "FTP Host Name"
msgstr "FTP 主機名稱"

#: fields.php:25
msgid "The ftp-server hostname, ip or hostname."
msgstr "FTP 的主機名稱，可能是 IP 位址或是網址。"

#: fields.php:29
msgid "ex: 123.123.123.123 or domain.com"
msgstr "例如：123.123.123.123 或是 domain.com"

#: fields.php:33
